{
    "name": "wxss-peek",
    "displayName": "wxss-peek",
    "publisher": "htprimer",
    "description": "jump to wxss definition",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.1.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:wxml"
    ],
    "main": "./extension",
    "contributes": {
        "languages": [
            {
                "id": "wxml",
                "extensions": [".wxml"]
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },    
    "repository": {
        "type": "git",
        "url": "https://github.com/htprimer/wxss-peek.git"
    }
}
